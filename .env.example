# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_CHANNEL_IDS=123456789012345678,987654321098765432  # Comma-separated channel IDs

# Synology NAS Configuration
SYNOLOGY_HOST=192.168.1.100  # Your NAS IP address
SYNOLOGY_PORT=5000  # Default DSM port (5000 for HTTP, 5001 for HTTPS)
SYNOLOGY_USERNAME=your_nas_username
SYNOLOGY_PASSWORD=your_nas_password
SYNOLOGY_USE_HTTPS=false  # Set to true if using HTTPS

# Download Configuration
DOWNLOAD_DESTINATION=downloads/discord-media  # Destination folder on NAS